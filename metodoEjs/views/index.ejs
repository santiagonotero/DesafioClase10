<html>
    <%- include('layout/base') %>
<body>
    <nav>
        <ul>
            <a href="/"><li>Inicio</li></a>
            <a href="/add"><li>Agregar objeto</li></a>
        </ul>
    </nav>
        <div class="container">
            <p>Listado de productos</p>
            <% listado %> 
            <table>
                <thead>
                    <tr>
                        <td>Art√≠culo</td>
                        <td>Precio</td>
                        <td>Imagen</td>
                    </tr>
                </thead>
                <tbody>
                    <% listado.forEach(function(item) { %>
                        <tr>
                            <td><%= item.title %></td>
                            <td><%= item.price %></td>
                            <td><img src=<%= item.thumbnail %> alt=""></td>
                        </tr>
                    <% }) %>
                </tbody>
            </table>
        </div>
</body>
</html>

